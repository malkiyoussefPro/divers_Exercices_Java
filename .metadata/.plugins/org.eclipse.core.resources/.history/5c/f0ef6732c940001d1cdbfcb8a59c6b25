public class Ejercicio3_8 {

	public static void main(String[] args) {
		
		int a=5;
		int b=9;
		int c=3;
		int d=4;
		int e;
		
		System.out.println("- El resultado de la operacion (2-a * b+c): "+ (e=2-a * b+c));
		System.out.println("- El resultado de la operacion (2-a * b+c): "+ (e=(2-a) * b+c));
		System.out.println("- El resultado de la operacion (2-a * b+c): "+ (e=2-a * b+c));
		System.out.println("- El resultado de la operacion (2-a * b+c): "+ (e=2-a * b+c));

	}

}
