package Ejercicios_3_2;

public class Ejercicios_3_C_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean   X, Y , Z;
		
		 X = true; 
		 Y =false ;	 
		 Z = true;
		
		/*
		 * 	
a) (X&&Y) || (X && Z) 		b) (X || !Y)&& (!X || Z)

c) X || Y && Z 			d) !(X || Y) && Z

e) X || Y || X && !Z && !Y 	f) !X || !Y || Z && X && !Y
		 */		
		System.out.println((X&&Y) || (X && Z));
		

	}

}
