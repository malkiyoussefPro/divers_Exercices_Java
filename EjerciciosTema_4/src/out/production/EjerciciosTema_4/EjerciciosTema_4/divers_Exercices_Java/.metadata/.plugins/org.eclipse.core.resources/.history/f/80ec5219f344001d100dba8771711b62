package Ejercicios_3_2;

public class Ejercicio3_16 {

	public static void main(String[] args) {
		// Declarar un array y rellenarlo
		
		double []notasEstudiante = {8.5,7.2,6.5};
		double mediaPrimerTrimestre;
		mediaPrimerTrimestre= (notasEstudiante[0]+notasEstudiante[1]+notasEstudiante[2])/3;
		System.out.println(mediaPrimerTrimestre);
	}

}
