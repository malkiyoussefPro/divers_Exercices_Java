package Ejercicios_tema3;

public class Ejercicio3_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int m = 23;
		int n = 12;
		int resultat=m+n;
		System.out.println(resultat);
		resultat=resultat-5;
		System.out.println(resultat);
		resultat=resultat*m;
		System.out.println(resultat);
		resultat=resultat/n;
		System.out.println(resultat);
		resultat=resultat+30;
		System.out.println(resultat);
	}

}
