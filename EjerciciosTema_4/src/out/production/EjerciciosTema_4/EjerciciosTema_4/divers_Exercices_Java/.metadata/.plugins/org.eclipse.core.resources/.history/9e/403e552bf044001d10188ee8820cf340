package Ejercicios_3_2;

public class Ejercicio3_15 {

	public static void main(String[] args) {
		// Declara y rellenar el array
		double [] arrayReales  =  {12.6,6.6,15.8,7.9,4.8,9.7};
		double suma =0;
		suma=suma+arrayReales[0]+arrayReales[2]+arrayReales[4];
		System.out.println(suma);
	}

}
