package Ejercicios_3_2;

public class Ejercicios_3_C_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 10;
		System.out.println( x );
		x ++;
		System.out.println ( x );
		System.out.println(++ x );
		System.out.println( x ++);
		System.out.println( x );
		System.out.println( x ++);
		System.out.println (++ x );
		System.out.println(++ x );
		++ x ;
		x ++;
		System.out.println(++ x );
		System.out.println( x ++);
		System.out.println(++ x );
		System.out.println();

	}

}
