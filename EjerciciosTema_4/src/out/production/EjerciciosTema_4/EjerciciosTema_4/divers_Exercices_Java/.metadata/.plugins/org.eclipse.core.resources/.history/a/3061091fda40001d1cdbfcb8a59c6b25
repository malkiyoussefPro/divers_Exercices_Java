package Ejercicios_Tema_3;

public class VariableBooleana {

	public static void main(String[] args) {
		
		int a=12;
		int b=23;
		boolean resultat=a>b;
		System.out.println(resultat);
		
		
		
		// - El valor mostrado es/: True
		// - El valor indica que la operacion es correcta o sea que el variable a que tiene un valor de 26 es mayor que b que tiene un valor de 13.  
		
		resultat=a<b;
		System.out.println(resultat);
		// - El valor mostrado es/: False
		// - El valor indica que la operacion no es correcta o sea que el variable a que tiene un valor de b es mayor que b que tiene un valor de 13.  
		int c=-5;
		System.out.println( c =a+b);
	}

}
